<BundleInfo name="ChimeraX-IHM" version="1.0"
	    package="chimerax.ihm"
  	    minSessionVersion="1" maxSessionVersion="1">

  <Author>UCSF RBVI</Author>
  <Email>chimerax@cgl.ucsf.edu</Email>
  <URL>https://www.rbvi.ucsf.edu/chimerax/</URL>

  <Synopsis>Integrative Hybrid Models file reader</Synopsis>
  <Description>Support Integrative Hybrid Models.</Description>

  <Categories>
    <Category name="Higher-Order Structure"/>
  </Categories>

  <Dependencies>
    <Dependency name="ChimeraX-Core" version=">=0.9"/>
    <Dependency name="ChimeraX-Atomic" version=">=1.0"/>
    <Dependency name="ChimeraX-Geometry" version=">=1.0"/>
    <Dependency name="ChimeraX-Map" version=">=1.0"/>
  </Dependencies>

  <Providers manager="data formats">
    <Provider name="IHM" category="Higher-order structure" suffixes=".ihm"
		reference_url="https://pdb-dev.wwpdb.org/about/"
		synopsis="Integrative Hybrid Models" />
  </Providers>

  <Providers manager="open command">
    <Provider name="IHM" want_path="true" />
    <Provider name="pdbdev" type="fetch" format_name="IHM" />
  </Providers>

  <Classifiers>
    <PythonClassifier>Development Status :: 2 - Pre-Alpha</PythonClassifier>
    <PythonClassifier>License :: Free for non-commercial use</PythonClassifier>
    <ChimeraXClassifier>DataFormat :: IHM :: :: Higher-Order Structure :: .ihm :: :: https://pdb-dev.wwpdb.org/about/ :: :: :: Integrative Hybrid Models data</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: IHM :: ihm :: true :: ensembles:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Fetch :: pdbdev :: IHM :: pdbdev :: 10 :: true</ChimeraXClassifier>
  </Classifiers>

</BundleInfo>
