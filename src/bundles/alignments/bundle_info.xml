<BundleInfo name="ChimeraX-Alignments" version="1.0" package="chimerax.seqalign"
  	    minSessionVersion="1" maxSessionVersion="1">

  <!-- Additional information about bundle source -->
  <Author>UCSF RBVI</Author>
  <Email>chimerax@cgl.ucsf.edu</Email>
  <URL>https://www.rbvi.ucsf.edu/chimerax/</URL>

  <!-- Synopsis is a one-line description
       Description is a full multi-line description -->
  <Synopsis>Sequence alignment support</Synopsis>
  <Description>Sequence alignment support</Description>

  <!-- Categories is a list where this bundle should appear -->
  <Categories>
    <Category name="Sequence"/>
  </Categories>

  <!-- Dependencies on other ChimeraX/Python packages -->
  <Dependencies>
    <Dependency name="ChimeraX-Core" version=">=0.9"/>
    <Dependency name="ChimeraX-Atomic" version=">=1.0"/>
    <Dependency name="ChimeraX-UI" version=">=1.0"/>
    <Dependency name="ChimeraX-DataFormats" version=">=1.0"/>
  </Dependencies>

  <Managers>
    <Manager name="alignments"/>
  </Managers>

  <Providers manager="data formats">
    <Provider name="Clustal ALN" nicknames="aln,clustal" synopsis="Clustal ALN sequence"
		category="Sequence" suffixes=".aln,.clustal,.clustalw,.clustalx"
		encoding="utf-8" />
    <Provider name="FASTA" synopsis="FASTA sequence" category="Sequence"
		suffixes=".fa,.fasta,.afasta,.afa"
		reference_url="https://en.wikipedia.org/wiki/FASTA_format" encoding="utf-8" />
    <Provider name="HSSP" synopsis="HSSP sequence" category="Sequence"
		suffixes=".hssp" encoding="utf-8" />
    <Provider name="GCG Multiple Sequence File" synopsis="MSF sequence" category="Sequence"
		nicknames="msf" suffixes=".msf" encoding="utf-8" />
    <Provider name="Pfam" synopsis="Pfam sequence" category="Sequence"
		suffixes=".selex,.slx,.pfam" encoding="utf-8" />
    <Provider name="NBRF/PIR" synopsis="PIR sequence" category="Sequence" nicknames="pir"
		reference_url="http://www.bioinformatics.nl/tools/crab_pir.html"
		suffixes=".ali,.pir" encoding="utf-8" />
    <Provider name="GCG Rich Sequence" synopsis="RSF sequence" category="Sequence"
		nicknames="rsf" suffixes=".rsf" encoding="utf-8" />
    <Provider name="Stockholm" synopsis="Stockholm sequence" category="Sequence"
		reference_url="https://en.wikipedia.org/wiki/Stockholm_format"
		suffixes=".sth,.sto" encoding="utf-8" />
  </Providers>

  <Providers manager="open command">
    <Provider name="Clustal ALN" />
    <Provider name="FASTA" />
    <Provider name="HSSP" />
    <Provider name="GCG Multiple Sequence File" />
    <Provider name="Pfam" />
    <Provider name="NBRF/PIR" />
    <Provider name="GCG Rich Sequence" />
    <Provider name="Stockholm" />
  </Providers>

  <Providers manager="save command">
    <Provider name="Clustal ALN" />
    <Provider name="FASTA" />
    <Provider name="NBRF/PIR" />
  </Providers>

  <Classifiers>
    <!-- Development Status should be compatible with bundle version number -->
    <PythonClassifier>Development Status :: 2 - Pre-Alpha</PythonClassifier>
    <PythonClassifier>License :: Free for non-commercial use</PythonClassifier>
    <ChimeraXClassifier>DataFormat :: FASTA :: :: Sequence alignment :: .fa,.fasta,.afasta,.afa :: :: https://en.wikipedia.org/wiki/FASTA_format :: :: :: FASTA format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: MSF :: :: Sequence alignment :: .msf :: :: :: :: :: GCG MSF (multiple sequence file) format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: PIR :: :: Sequence alignment :: .ali,.pir :: :: http://www.bioinformatics.nl/tools/crab_pir.html :: :: :: PIR (NBRF) format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: ALN :: :: Sequence alignment :: .aln,.clustal,.clustalw,.clustalx :: :: :: :: :: Clustal ALN format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: HSSP :: :: Sequence alignment :: .hssp :: :: :: :: :: HSSP format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: Pfam :: :: Sequence alignment :: .selex,.slx,.pfam :: :: :: :: :: Selex (Pfam) format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: RSF :: :: Sequence alignment :: .rsf :: :: :: :: :: GCG RSF format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: Stockholm :: :: Sequence alignment :: .sth,.sto :: :: https://en.wikipedia.org/wiki/Stockholm_format :: :: :: Stockholm format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: FASTA :: fasta :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: MSF :: msf :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: PIR :: pir :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: ALN :: clustal :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: HSSP :: hssp :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: Pfam :: selex :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: RSF :: rsf :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Open :: Stockholm :: stockholm :: true :: alignment:Bool,auto_associate:Bool</ChimeraXClassifier>
    <ChimeraXClassifier>Save :: ALN :: aln :: true :: alignment:Alignment</ChimeraXClassifier>
    <ChimeraXClassifier>Save :: FASTA :: fasta :: true :: alignment:Alignment</ChimeraXClassifier>
    <ChimeraXClassifier>Save :: PIR :: pir :: true :: alignment:Alignment</ChimeraXClassifier>
    <ChimeraXClassifier>Command :: sequence :: Sequence alignment :: control/open sequence alignments</ChimeraXClassifier>
  </Classifiers>

</BundleInfo>
