<BundleInfo name="ChimeraX-MapData" version="1.0"
	    package="chimerax.map.data"
  	    minSessionVersion="1" maxSessionVersion="1">

  <Author>UCSF RBVI</Author>
  <Email>chimerax@cgl.ucsf.edu</Email>
  <URL>https://www.rbvi.ucsf.edu/chimerax/</URL>

  <Synopsis>Volume data file formats</Synopsis>
  <Description>Open and save volume data.</Description>

  <Categories>
    <Category name="Volume Data"/>
  </Categories>

  <Dependencies>
    <Dependency name="ChimeraX-Core" version=">=0.9"/>
    <Dependency name="ChimeraX-Geometry" version=">=1.0"/>
    <Dependency name="ChimeraX-DataFormats" version=">=1.0"/>
  </Dependencies>

  <Providers manager="data formats">
    <Provider name="amira" synopsis="Amira mesh" category="Volume data"
		suffixes=".am" />
    <Provider name="apbs" synopsis="APBS potential" category="Volume data"
		suffixes=".dx" />
    <Provider name="brix" synopsis="BRIX density map" category="Volume data"
		suffixes=".brix" />
    <Provider name="ccp4" synopsis="CCP4 density map" category="Volume data"
		suffixes=".ccp4,.map" />
    <Provider name="cmap" synopsis="Chimera map" category="Volume data"
		suffixes=".cmap,.cmp" />
    <Provider name="xplor" synopsis="CNS or XPLOR density map" category="Volume data"
		suffixes=".cns,.xplor" />
    <Provider name="delphi" synopsis="DelPhi or GRASP potential" category="Volume data"
		suffixes=".phi" />
    <Provider name="deltavision" synopsis="DeltaVision map" category="Volume data"
		suffixes=".dv" />
    <Provider name="dns6" synopsis="DSN6 density map" category="Volume data"
		suffixes=".omap" />
    <Provider name="dock" synopsis="DOCK scoring grid" category="Volume data"
		suffixes=".bmp,.cnt,.nrg" />
    <Provider name="emanhdf" synopsis="EMAN HDF map" category="Volume data"
		suffixes=".hdf,.hdf5,.h5" />
    <Provider name="cube" synopsis="Gaussian cube grid" category="Volume data"
		suffixes=".cube,.cub" />
    <Provider name="gopenmol" synopsis="gOpenMol grid" category="Volume data"
		suffixes=".plt" />
    <Provider name="hdf" synopsis="HDF map" category="Volume data" />
    <Provider name="images" synopsis="Image stack" category="Volume data"
		suffixes=".tif,.tiff,.png,.pgm" />
    <Provider name="imagic" synopsis="IMAGIC density map" category="Volume data"
		suffixes=".hed,.img" />
    <Provider name="ims" synopsis="Imaris map" category="Volume data"
		suffixes=".ims" />
    <Provider name="imodmap" synopsis="IMOD map" category="Volume data"
		suffixes=".rec" />
    <Provider name="macmolplt" synopsis="MacMolPlt grid" category="Volume data"
		suffixes=".mmp" />
    <Provider name="mrc" synopsis="MRC density map" category="Volume data"
		suffixes=".mrc" />
    <Provider name="netcdf" synopsis="NetCDF generic array" category="Volume data"
		suffixes=".nc" />
    <Provider name="priism" synopsis="Priism microscope image" category="Volume data"
		suffixes=".xyzw,.xyzt" />
    <Provider name="profec" synopsis="PROFEC free energy grid" category="Volume data"
		suffixes=".profec" />
    <Provider name="pif" synopsis="Purdue image format" category="Volume data"
		suffixes=".pif" />
    <Provider name="situs" synopsis="SITUS map file" category="Volume data"
		suffixes=".sit,.situs" />
    <Provider name="spider" synopsis="SPIDER volume data" category="Volume data"
		suffixes=".spi,.vol" />
    <Provider name="tom_em" synopsis="TOM toolbox EM density map" category="Volume data"
		suffixes=".em" />
    <Provider name="uhbd" synopsis="UHBD grid, binary" category="Volume data"
		suffixes=".grd" />
  </Providers>

  <Classifiers>
    <PythonClassifier>Development Status :: 2 - Pre-Alpha</PythonClassifier>
    <PythonClassifier>License :: Free for non-commercial use</PythonClassifier>
  </Classifiers>

</BundleInfo>
