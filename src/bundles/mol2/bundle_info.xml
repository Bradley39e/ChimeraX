<BundleInfo name="ChimeraX-Mol2" version="1.0"
	    package="chimerax.atomic.mol2"
  	    minSessionVersion="1" maxSessionVersion="1">

  <Author>UCSF RBVI</Author>
  <Email>chimerax@cgl.ucsf.edu</Email>
  <URL>https://www.rbvi.ucsf.edu/chimerax/</URL>

  <Synopsis>Mol2 reader/writer</Synopsis>
  <Description>Sybyl Mol2 input/output.</Description>

  <Categories>
    <Category name="I/O"/>
  </Categories>

  <Dependencies>
    <Dependency name="ChimeraX-Core" version=">=0.9"/>
    <Dependency name="ChimeraX-Atomic" version=">=1.0"/>
    <Dependency name="ChimeraX-ChemGroup" version=">=1.0"/>
    <Dependency name="ChimeraX-Geometry" version=">=1.0"/>
  </Dependencies>

  <Providers manager="data formats">
    <Provider name="Mol2" synopsis="Sybyl Mol2" category="Molecular structure"
		suffixes=".mol2" encoding="utf-8" />
  </Providers>

  <Providers manager="save command">
    <Provider name="Mol2" />
  </Providers>

  <Classifiers>
    <PythonClassifier>Development Status :: 2 - Pre-Alpha</PythonClassifier>
    <PythonClassifier>License :: Free for non-commercial use</PythonClassifier>
    <ChimeraXClassifier>DataFormat :: Sybyl Mol2 :: mol2 :: Molecular structure :: .mol2 :: :: :: :: :: Sybyl Mol2 format :: utf-8</ChimeraXClassifier>
    <ChimeraXClassifier>Save :: Sybyl Mol2 :: mol2 :: true :: models:Models,atoms:Atoms,anchor:Atoms,rel_model:Model,sybyl_hyd_naming:Bool,combine_models:Bool,skip_atoms:Atoms,res_num:Bool,gaff_type:Bool</ChimeraXClassifier>
  </Classifiers>

</BundleInfo>
