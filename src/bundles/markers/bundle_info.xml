<BundleInfo name="ChimeraX-Markers" version="1.0"
	    package="chimerax.markers"
  	    minSessionVersion="1" maxSessionVersion="1" customInit="true">

  <Author>UCSF RBVI</Author>
  <Email>chimerax@cgl.ucsf.edu</Email>
  <URL>https://www.rbvi.ucsf.edu/chimerax/</URL>

  <Synopsis>Place markers on density maps</Synopsis>
  <Description>Mouse mode to place markers on density maps.</Description>

  <DataFiles>
    <DataFile>icons/*.png</DataFile>
  </DataFiles>

  <Categories>
    <Category name="Volume Data"/>
  </Categories>

  <Dependencies>
    <Dependency name="ChimeraX-Core" version=">=0.9"/>
    <Dependency name="ChimeraX-Atomic" version=">=1.0"/>
    <Dependency name="ChimeraX-Geometry" version=">=1.0"/>
    <Dependency name="ChimeraX-MouseModes" version=">=1.0"/>
    <Dependency name="ChimeraX-UI" version=">=1.0"/>
  </Dependencies>

  <Classifiers>
    <PythonClassifier>Development Status :: 2 - Pre-Alpha</PythonClassifier>
    <PythonClassifier>License :: Free for non-commercial use</PythonClassifier>
    <ChimeraXClassifier>Tool :: Marker Placement :: Volume Data :: Place markers on density maps</ChimeraXClassifier>
    <ChimeraXClassifier>DataFormat :: Marker model :: markers :: Generic 3D objects :: .cmm :: :: http://rbvi.ucsf.edu/chimerax/docs/user/markers.html#markerfiles :: :: :: Markers and links </ChimeraXClassifier>
    <ChimeraXClassifier>Open :: Marker model :: cmm ::</ChimeraXClassifier>
    <ChimeraXClassifier>Save :: Marker model :: cmm :: true :: models:Models</ChimeraXClassifier>
    <ChimeraXClassifier>Command :: marker :: General :: Place markers</ChimeraXClassifier>
  </Classifiers>

</BundleInfo>
